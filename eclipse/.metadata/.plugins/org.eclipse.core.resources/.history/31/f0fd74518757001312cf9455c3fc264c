package net.minecraft.src;

import org.greencubes.items.decor.DecorItemQuality;

public class ItemPackedNamedItem extends ItemGiftBagNy {

	protected static final String[] packedNames = new String[] {"cherryStick","diamondPickaxe","diamondShovel","diamondAxe","decorBow"};
	
	protected ItemPackedNamedItem(int i) {
		super(i);
	}

	@Override
	public DecorItemQuality getDecorQuality() {
		return DecorItemQuality.NAMED;
	}

	@Override
	public boolean noDrop() {
		return true;
	}

	@Override
	public String getItemNameIS(ItemStack itemstack) {
		if(itemstack.getItemDamage() >= packedNames.length)
			return null;
		return (new StringBuilder()).append(super.getItemName()).append(".").append(packedNames[itemstack.getItemDamage()]).toString();
	}

}
