package net.minecraft.src;

public class ItemArmor extends ItemWearable {
	
	private final EnumArmorMaterial material;
	public final int armorType;
	public final int damageReduceAmount;

	public ItemArmor(int id, EnumArmorMaterial material, int slot, int grade) {
		super(id, slot);
		material = material;
		armorType = k;
		damageReduceAmount = enumarmormaterial.damageReduction(k);
		setMaxDamage(enumarmormaterial.durability(k));
	}

}
